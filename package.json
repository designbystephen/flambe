{
  "name": "flambe",
  "version": "1.1.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn compile && yarn bundle && yarn package",
    "build:start": "cross-env ZIP=false yarn build && yarn start",
    "compile": "babel src --out-dir .build",
    "bundle": "browserify .build/popup.js -o .build/popup-compiled.js",
    "package": "gulp",
    "start": "web-ext run --source-dir=./dist --firefox=firefoxdeveloperedition --browser-console"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "browserify": "^16.2.3",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-minify": "^3.1.0",
    "gulp-zip": "^4.2.0",
    "handlebars": "^4.1.0",
    "web-ext": "^2.9.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5"
  }
}
